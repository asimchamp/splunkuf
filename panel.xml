| rest /servicesNS/-/-/saved/searches
  | search disabled=0 is_scheduled=1
  | eval object_type=if(is_report=1,"Report","Saved Search")
  | eval schedule=coalesce(cron_schedule, schedule)
  | table title app owner object_type schedule
| append
[
  | rest /servicesNS/-/-/datamodel/model
  | rex field=acceleration "^\{\"enabled\"\:(?<is_enabled>[^\,]+)"
  | search is_enabled=true
  | eval object_type="Data Model (Accelerated)"
  | table title app owner object_type
]
| stats count
